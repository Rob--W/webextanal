{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2019
    },
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "strict": [2, "global"],
        "max-len": [2, {
            "code": 100,
            "ignoreUrls": true,
            "ignoreRegExpLiterals": true
        }],
        "no-console": [0]
    }
}
